BASE <http://example.com/> 
PREFIX ite: <http://w3id.org/sparql-generate/iter/>
PREFIX fun: <http://w3id.org/sparql-generate/fn/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX mail: <http://example.com/mail#>
PREFIX note: <http://example.com/note/>
PREFIX ex: <http://example.com/>
PREFIX idpo: <http://w3id.org/idpo#> 
PREFIX prov: <http://www.w3.org/ns/prov#> 
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
PREFIX bpmn2: <http://www.reiter.at/ontology/bpmn2.0#>

GENERATE { 
    <http://example.com/note/{?sFId}> a bpmn2:SequenceFlow ;
        bpmn2:sourceRef ?sR ;
        bpmn2:targetRef ?tR .
}
SOURCE <http://example.org/document#document0.xml> AS ?source
ITERATOR ite:XPath(?source, "//sequenceFlow" ) AS ?sequenceFlow 
WHERE {
     BIND( fun:XPath(?sequenceFlow, "/sequenceFlow/@id") AS ?sFId)
     BIND( fun:XPath(?sequenceFlow, "/sequenceFlow/@sourceRef/") AS ?sR)
     BIND( fun:XPath(?sequenceFlow, "/sequenceFlow/@targetRef/") AS ?tR)
} 