BASE <http://example.com/> 
PREFIX ite: <http://w3id.org/sparql-generate/iter/>
PREFIX fun: <http://w3id.org/sparql-generate/fn/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX mail: <http://example.com/mail#>
PREFIX note: <http://example.com/note/>
PREFIX ex: <http://example.com/>
PREFIX idpo: <http://w3id.org/idpo#> 
PREFIX prov: <http://www.w3.org/ns/prov#> 
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
PREFIX bpmn2: <http://www.reiter.at/ontology/bpmn2.0#>

GENERATE { 
    <http://example.com/note/{?startEventId}> a bpmn2:StartEvent ;
        bpmn2:incoming  <http://example.com/note/{?ic}>.
}
SOURCE <http://example.org/document#document0.xml> AS ?source
ITERATOR ite:XPath(?source, "//endEvent" ) AS ?endEvent 
WHERE {
     BIND( fun:XPath(?endEvent, "/endEvent/@id") AS ?endEventId)
     BIND( fun:XPath(?endEvent, "/endEvent/outgoing/text()") AS ?c)
} 